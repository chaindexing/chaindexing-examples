{
  "name": "chaindexing-examples-typescript",
  "version": "0.1.0",
  "description": "TypeScript examples for Chaindexing - indexing EVM blockchain smart contracts",
  "private": true,
  "scripts": {
    "build": "tsc",
    "nfts": "ts-node nfts/src/main.ts",
    "uniswap": "ts-node uniswap/src/main.ts",
    "dev:nfts": "nodemon nfts/src/main.ts",
    "dev:uniswap": "nodemon uniswap/src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test-status": "node test-status.js",
    "db:up": "docker-compose up -d",
    "db:down": "docker-compose down"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "postgres": "^3.3.5",
    "drizzle-orm": "^0.29.1"
  },
  "devDependencies": {
    "@types/node": "^20.4.8",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 